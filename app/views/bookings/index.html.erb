<div class="banner">
   <h3>Bookings</h3>
</div>

<div class="container my-5">
  <div class="row">
    <div class="col-md-6">
      <div class="card">
        <div class="card-header">
          Animal Details
        </div>
        <div class="card-body">
          <% @bookings.each do |booking| %>
            <h2><%= booking.animal.name %></p></h2>
            <p>From <%= booking.start_date %> to <%= booking.end_date %></p>
          <% end %>
        </div>
      </div>
    </div>
  </div>
</div>
